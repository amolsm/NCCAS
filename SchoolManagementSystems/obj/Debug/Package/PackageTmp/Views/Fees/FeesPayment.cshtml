                                    dataType: 'json',
                                    data: { feesid: feesdetails.Feesid },
                                    success: function (data) {
                                        if (data != "" && data != null) {
                                            $("#feesdetails").html('');
                                            var html = "";
                                            html = "<fieldset><legend>Fees Setup Details</legend>";
                                            $.each(data, function (i, dt) {
                                                html += "<label class = 'col-md-6 form_out'>" + dt.FeesNm + " :</label><label  class = 'col-md-5 form-control form_in'>" + dt.Fees + "</label>";
