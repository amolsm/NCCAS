@model Entity.positionviewmodel
<div id="result" class="table-responsive">
    <table class="table tb-parshav tablesorter">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.posid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.posname)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.status)
                </th>
                @*<th>
                    @Html.DisplayNameFor(model => model.academicyear)
                </th>*@
               <th class="edt">
                    @Html.DisplayName("Action")
                </th>
            </tr>
            </thead>
        <tbody>
            @foreach (var item in Model._positionlist)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.posid)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.posname)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.status)
                    </td>
                    @*<td>
                        @Html.DisplayFor(modelItem => item.academicyear)
                    </td>*@
                    <td>
                        <input type="button" id="@item.posid" class="btn btn-default btn_edit" value="Edit" />
                        @*<input type="button" id="@item.posid" class="btn btn-default btn_delete" value="Delete" />*@
                    </td>
                </tr>
            }
            </tbody>
</table>
</div>