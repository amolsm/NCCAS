@model Entity.Subjectviewmodel
<div id="result" class="table-responsive">
    <table class="table tb-parshav tablesorter">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Subjectid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Classid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SubjectNm)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Marks)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Pass_Marks)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.status)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.academicyear)
                </th>
                <th class="edt">
                    @Html.DisplayName("Action")
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model._Subjectlist)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Subjectid)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Classnm)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SubjectNm)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Marks)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Pass_Marks)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.status)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.academicyear)
                    </td>
                    <td>
                        <input type="button" id="@item.Subjectid" class="btn btn-default btn_edit edit" value="Edit" />
                        @*<input type="button" id="@item.Subjectid" class="btn btn-default btn_delete delete" value="Delete" />*@
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
