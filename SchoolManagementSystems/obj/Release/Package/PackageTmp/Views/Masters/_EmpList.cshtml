@model Entity.employeeviewmodel
<div id="result" class="table-responsive">
    <table class="table table-striped table-bordered bootstrap-datatable datatable">
        <thead>
            <tr>
            <th>Name</th>  
            <th>Emailid</th>  
            <th>PhoneNo</th>  
            <th>MobileNo</th>  
            <th>Gender</th>  
            <th>DateOfJoin</th>  
            <th>Typeid</th>   
                <th class="edt">
                    @Html.DisplayName("Action")
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.userdetails)
            {
                <tr>
                     <td>
                       @item.Empid
                    </td>
                    <td>
                       @item.FirstName 
                    </td>
                    <td>
                       @item.Emailid 
                    </td>
                    <td>
                       @item.PhoneNo 
                    </td>
                    <td>
                       @item.MobileNo 
                    </td>
                    <td>
                        @item.Gender 
                    </td>
                    
                    <td>
                       @item.DateOfJoin 
                    </td>
                    <td>
                       @item.Typeid 
                    </td>
                    <td>
                        <input type="button" id="@item.Empid" class="btn btn-default edit" value="Edit" />
                        @*<input type="button" id="@item.Empid" class="btn btn-default delete" value="Delete" />*@
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
