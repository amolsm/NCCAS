@model Entity.studentdivisionmodel
<div id="result" class="table-responsive">
    <table class="table table-striped table-bordered bootstrap-datatable datatable">
        <thead>
            <tr>
            <th>Student Name</th>  
            <th>Division</th>  
            <th>Course Name</th>  
            <th>Department Name</th>  
            <th>Year</th>  
            <th>Created By</th>  
            <th>Status</th>   
                <th class="edt">
                    @Html.DisplayName("Action")
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.userdetails)
            {
                <tr>
                     <td>
                       @item.id
                    </td>
                    <td>
                       @item.studentid 
                    </td>
                    <td>
                       @item.divisionid 
                    </td>
                    <td>
                       @item.courseid 
                    </td>
                    <td>
                       @item.departmentid 
                    </td>
                    <td>
                        @item.yearid 
                    </td>
                    
                    <td>
                       @item.createdby 
                    </td>
                    <td>
                       @item.IsActive 
                    </td>
                    <td>
                        <input type="button" id="@item.id" class="btn btn-default edit" value="Edit" />
                        @*<input type="button" id="@item.Empid" class="btn btn-default delete" value="Delete" />*@
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
