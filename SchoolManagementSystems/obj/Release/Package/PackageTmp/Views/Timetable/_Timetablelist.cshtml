@model Entity.timetableviewmodel
<div id="result" class="table-responsive">
    <table class="table table-striped table-bordered bootstrap-datatable datatable">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("ID")
            </th>
            <th>
                @Html.DisplayName("Class")
            </th>
            <th>
                @Html.DisplayName("Day")
            </th>
            <th>
                @Html.DisplayName("LecNo.")
            </th>
            <th>
                @Html.DisplayName("Subject")
            </th>
            <th>
                @Html.DisplayName("Start Time")
            </th>
            <th>
                @Html.DisplayName("End Time")
            </th>
            <th>
                @Html.DisplayName("Teacher")
            </th>
            <th class="edt">
                Action
            </th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model._Timetablelist)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Tid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Classid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Day)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LecNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Subjectid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LecTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LecETime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Empid)
                </td>
                <td>
                    <input type="button" id="@item.Tid" class="btn btn-default btn_edit edit" value="Edit" />
                    @*<input type="button" id="@item.Tid" class="btn btn-default btn_delete delete" value="Delete" />*@
                </td>
            </tr>
        }
        </tbody>
    </table>
</div>
