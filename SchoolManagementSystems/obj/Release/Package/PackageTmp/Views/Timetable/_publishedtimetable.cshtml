@model Entity.timetableviewmodel
<div id="result" class="table-responsive">
    <table class="table tb-parshav">
        <tr>
            <th>
                @Html.DisplayName("Class")
            </th>            
            <th>
                @Html.DisplayName("LecNo.")
            </th>
            <th>
                @Html.DisplayName("Mon")
            </th>
            <th>
                @Html.DisplayName("Tue")
            </th>
            <th>
                @Html.DisplayName("Wed")
            </th>
            <th>
                @Html.DisplayName("Thur")
            </th>
            <th>
                @Html.DisplayName("Fri")
            </th>
            <th>
                @Html.DisplayName("Sat")
            </th>
        </tr>
        @foreach (var item in Model._Timetablelistpivot)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Classnm)
                </td>                
                <td>
                    @Html.DisplayFor(modelItem => item.LecNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Monday)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tuesday)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Wednesday)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Thursday)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Friday)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Saturday)
                </td>                
            </tr>
        }
    </table>
</div>
