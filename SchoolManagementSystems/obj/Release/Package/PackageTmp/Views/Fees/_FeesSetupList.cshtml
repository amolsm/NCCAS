@model Entity.feesviewmodel
<div id="result" class="table-responsive">
    <table class="table table-striped table-bordered bootstrap-datatable datatable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Feesid)
            </th>   
           
            <th>
                @Html.DisplayNameFor(model => model.receipttypeid)
            </th>
          
            <th>
                @Html.DisplayNameFor(model => model.TotalFees)
            </th>
            <th class="edt">
                @Html.DisplayName("Action")
            </th>
        </tr>
        </thead>
                                    <tbody>
        @foreach (var item in Model._Feessetuplist)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Feesid)
                </td>
             
                <td>
                    @Html.DisplayFor(modelItem => item.R_Name)
                </td>
          
                <td>
                    @Html.DisplayFor(modelItem => item.TotalFees)
                </td>
                <td>
                    <input type="button" id="@item.Feesid" class="btn btn-default btn_edit" value="Edit" />
                   
                </td>
            </tr>
        }
        </tbody>
    </table>
</div>
