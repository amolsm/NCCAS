        $("#PrScTotalMarks").change(function () {
            var a = $("#PrScObtainMark").val();
            var b = $("#PrScTotalMarks").val();
            var sum = (a / b) * 100;
            $("#PrScPercentage").val(parseFloat(sum.toFixed(2)));
            if ((sum > 60) && (sum < 100)) {
                $("#Grade").val('A');
            }
            if ((sum > 41) && (sum < 59)) {
                $("#Grade").val('B');
            }
            if ((sum > 01) && (sum < 40)) {
                $("#Grade").val('C');
            }

