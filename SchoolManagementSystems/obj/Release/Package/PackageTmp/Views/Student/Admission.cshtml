        function ReloadCountry() {
            if ($("#Classid").val() == "") {
                ReloadState();
            }
            else {

                $("#DepartmentId").empty();
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("GetCourse")',
                    dataType: 'json',
                    data: { id: $("#Classid").val() },
                    success: function (data) {
                       if ($(data.courselist).length != 0) {
                            
                            var item = '<option value="">select Department</option>';
                            $.each(data.courselist, function (i, course) {
                                item += '<option value="' + course.Value + '">' + course.Text + '</option>';
                            });
                            $("#DepartmentId").append(item);
                        }
                        else {
                            var item = '<option value="">select Department</option>';
                            $("#DepartmentId").append(item);
                       }
                       if ($(data.coursetypelist).length != 0) {
                           var coursetype = data.coursetypelist;
                           switch (coursetype)
                           {
                               case 1:
                                   $('#hiddencourstype').val('1');
                                   $('#sscspancondition').hide();
                                   $('#hscspancondition').hide();
                                   $('#ugspancondition').hide();
                                   $('#pgspancondition').hide();
                                   $('#sscspan').show();
                                   $('#hscspan').hide();
                                   $('#ugspan').hide();
                                   $('#pgspan').hide();
                                   break;
                               case 2:
                                  
                                   $('#hiddencourstype').val('2');
                                   $('#sscspancondition').show();
                                   $('#hscspancondition').hide();
                                   $('#ugspancondition').hide();
                                   $('#pgspancondition').hide();
                                   $('#sscspan').hide();
                                   $('#hscspan').show();
                                   $('#ugspan').hide();
                                   $('#pgspan').hide();
                                   break;
                               case 3:
                                  
                                   $('#hiddencourstype').val('3');
                                   $('#sscspancondition').hide();
                                   $('#hscspancondition').show();
                                   $('#ugspancondition').hide();
                                   $('#pgspancondition').hide();
                                   $('#sscspan').hide();
                                   $('#hscspan').hide();
                                   $('#ugspan').show();
                                   $('#pgspan').hide();
                                   break;
                               default:
                                   $('#hiddencourstype').val('0');
                                   $('#sscspan').show();
                                   $('#hscspan').show();
                                   $('#ugspan').show();
                                   $('#pgspan').show();
                                   $('#sscspancondition').hide();
                                   $('#hscspancondition').hide();
                                   $('#ugspancondition').hide();
                                   $('#pgspancondition').hide();
                                  
                           }
                       }
                    },
                    error: function (ex) {
                        alert('Failed to retrieve course.' + ex);
                    }
                });
            }
            return false;
        }

