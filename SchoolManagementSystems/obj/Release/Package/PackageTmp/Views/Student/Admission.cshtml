        function ReloadRegisteartionNo() {
            if ($("#academicyear").val() == "" || $("#academicyear").val() == "0") {
                $("#StdRegNo").val('');
            }
            else {
                  $.ajax({
                    type: 'POST',
                    url: '@Url.Action("GetRegisterationNo")',
                    dataType: 'json',
                    data: {
                        acadmicyear:  $("#academicyear option:selected").text(),
                        deptid: $("#DepartmentId").val()
                    },
                    success: function (data) {
                        if (data != "") {

                            var accadmic = $("#academicyear option:selected").text();
                            var year = accadmic.substring(0, 4);
                            var dept = $("#DepartmentId option:selected").text();
                            var deptname = dept.substring(0, 3);
                            var count = parseInt(data) + 1;
                            var stdreg;
                            if ((count >= 0) && (count <= 9)) {
                                stdreg = (year + deptname.toUpperCase() + "0000" + count)
                            }
                            else if ((count >= 9) && (count <= 99)) {
                                stdreg = (year + deptname.toUpperCase() + "000" + count)
                            }
                            else if ((count >= 99) && (count <= 999)) {
                                stdreg = (year + deptname.toUpperCase() + "00" + count)
                            }
                            else if ((count >= 999) && (count <= 9999)) {
                                stdreg = (year + deptname.toUpperCase() + "0" + count)
                            }
                            else if ((count >= 9999) && (count <= 99999)) {
                                stdreg = (year + deptname.toUpperCase() + count)
                            }
                         
                            $("#StdRegNo").val(stdreg);
                        }
                        else {

                            $("#StdRegNo").val('');
                        }
                    },
                    error: function (ex) {
                        alert('Failed to retrieve course.' + ex);
                    }
                });
            }
            return false;
        }

