@model Entity.Categoryviewmodel
@{
    ViewBag.Title = "Category";
    Layout = "~/Views/shared/_SharedLayout.cshtml";
}
<div class="container">
    <div class="row">
        <div class="col-md-12" style="padding-top: 75px;margin-bottom: 90px;">
            <ul class="breadcrumb">
                <li>@Html.ActionLink("Home", "Login", "Login")</li>
                <li>@Html.ActionLink("Inventory Management", "Category", "Inventory", "")</li>
                <li class="active">Stock Report</li>
            </ul>
        </div>
        <div class="col-md-2">
            <div class="well blbg">
                <a href="~/Login/Login">Back to Home</a>
                @Html.Partial("_InventorySubMenu")
            </div>
        </div>
        <div class="col-md-9">
            <center>
                <table width="100%">
                    <tr>
                        <td>
                            <div>
                                <div class="well blbg" style="">
                                    @Html.Partial("_stockreport")
                                </div>
                            </div>
                        </td>
                    </tr>
                </table>
            </center>
        </div>
        <!-- table end -->
        @*</div>*@
    </div>
</div>
