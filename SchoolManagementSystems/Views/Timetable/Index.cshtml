@model Entity.timetableviewmodel
@{
    ViewBag.Title = "Timetable Management";
    Layout = "~/Views/shared/_SharedLayout.cshtml";
}
<script src="../../Scripts/jQuery.print.js" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        FillPermission();        
        $(".test").click(function () {
            $("#result").print({
                addGlobalStyles: true,
                stylesheet: null,
                rejectWindow: true,
                noPrintSelector: ".no-print",
                iframe: true,
                append: null,
                prepend: null
            });
        });

        $(".btn_edit").click(function () {
            var Tid = this.id;
            location.href = '/Timetable/NewTimetableInfo?Tid=' + Tid;
        });
        $(".btn_delete").click(function () {
            var Tid = this.id;
            var act = 'Delete';
            var flag = confirm('Are you sure you want to delete this Timetable?');
            if (flag) {
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("DMLTimetable")',
                    dataType: 'json',
                    data: { evt: act, id: Tid },
                    success: function () {
                    }
                });
            }
        });
        ReloadPaging();
    });
</script>
@Html.Hidden("Add", ViewData["Add"])
@Html.Hidden("Modify", ViewData["Modify"])
@Html.Hidden("View", ViewData["View"])
@Html.Hidden("Delete", ViewData["Delete"])
<div class="container-fluid-full">
             
		<div class="row-fluid">
                @Html.Partial("_TimetableSubMenu")
	<div id="content" class="span10">
        	<div class="row-fluid">
                
            
           
                    <ul class="breadcrumb">
                      <li><i class="icon-home"></i>@Html.ActionLink("Home", "Login", "Login")<i class="icon-angle-right"></i>
                 @Html.ActionLink("Timetable Management", "Index", "Student", "")<i class="icon-angle-right"></i></li>
                        <li class="active">Timetable History</li>
                        <div style="float: right;">
                            @using (Html.BeginForm())
                            {

                                @Html.TextBox("Search_Data", ViewBag.FilterValue as string)
                                <input type="submit" value="Find" />
                            }
                        </div>
                    </ul>
                    <div class="col-md-10" style="width: 100%;">
                        <div class="" style=" color: #23527c;">
                            <div class="col-md-4 col-md-offset-8">
                                <form class="navbar-form navbar-left pull-right" role="search" style="margin-right: -30px;">
                                <div class="form-group">
                                    <input type="button" class="test btn btn-default" value="print" />
                                    @*<input type="text" class="form-control ptext" placeholder="Search">*@
                                </div>
                                </form>
                            </div>
                            <!-- table start -->
                            <div id="result" class="table-responsive">
                                <table class="table tb-parshav tablesorter">
                                <thead>
                                    <tr>
                                        <th>
                                            @Html.DisplayName("ID")
                                        </th>
                                        <th>
                                            @Html.DisplayName("Class")
                                        </th>
                                        <th>
                                            @Html.DisplayName("Day")
                                        </th>
                                        <th>
                                            @Html.DisplayName("LecNo.")
                                        </th>
                                        <th>
                                            @Html.DisplayName("Subject")
                                        </th>
                                        <th>
                                            @Html.DisplayName("Start Time")
                                        </th>
                                        <th>
                                            @Html.DisplayName("End Time")
                                        </th>
                                        <th>
                                            @Html.DisplayName("Teacher")
                                        </th>
                                        @*<th>
                                                Action
                                            </th>                   *@
                                    </tr>
                                    </thead>
        <tbody>
                                    @foreach (var item in Model._Timetablelist)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Tid)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Classid)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Day)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.LecNo)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Subjectid)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.LecTime)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.LecETime)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Empid)
                                            </td>
                                            @* <td>
                                                    <input type="button" id="@item.Tid" class="btn btn-default btn_edit" value="Edit" />
                                                    <input type="button" id="@item.Tid" class="btn btn-default btn_delete" value="Delete" />
                                                </td>*@
                                        </tr>
                                    }
                                    </tbody>
                                </table>
                            </div>
                            <div id="pager" class="col-md-4 col-md-offset-8" style="position: none;">
                                    <img src="../../Content/images/first.png" style="width:19px;" class="first" />
                                    <img src="../../Content/images/prev.png"  style="width:19px;" class="prev" />
                                    <input type="text" style="width:50px;" disabled="disabled" class="pagedisplay" />
                                    <img src="../../Content/images/next.png"  style="width:19px;" class="next" />
                                    <img src="../../Content/images/last.png"  style="width:19px;" class="last" />
                                    <select style="border-color:Blue;" class="pagesize">
                                        <option selected="selected" value="10">10</option>
                                        <option value="15">15</option>
                                        <option value="20">20</option>
                                        <option value="25">25</option>
                                        <option value="50">50</option>
                                        <option value="100">100</option>
                                    </select>
                                </div>
                            <!-- table end -->
                        </div>
                    </div>
              
           </div>
        </div>
    </div>
</div>
