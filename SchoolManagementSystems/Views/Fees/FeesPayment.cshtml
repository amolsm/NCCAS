@{
    ViewBag.Title = "Fees Payment";
    Layout = "~/Views/shared/_SharedLayout.cshtml";
}
<style>
    @@media print {
        #print_btn {
            display: none;
        }

        #back_btn {
            display: none;
        }
    }

    .fee_receipt table td {
        font-size: 12px;
    }

    .date_fee {
        margin: 0px;
    }

    h5 {
        margin: 0px;
    }

    .table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
        padding: 4px;
    }
   
