@model Entity.otherpaymentviewmodel
<div id="result" class="table-responsive">
    <table class="table table-striped table-bordered bootstrap-datatable datatable">
        <thead>
            <tr>
                <th>
                    @Html.DisplayName("ID")
                </th>
                <th>
                    @Html.DisplayName("Receipt Type")
                </th>
                <th>
                    @Html.DisplayName("Name")
                </th>
                <th>
                    @Html.DisplayName("Place")
                </th>
                <th>
                    @Html.DisplayName("TotalFees")
                </th>
                <th>
                    @Html.DisplayName("PaymentAmt")
                </th>
                <th>
                    @Html.DisplayName("PaymentDate")
                </th>
                <th>
                    @Html.DisplayName("Remark")
                </th>

               
                <th>
                    @Html.DisplayName("Print Receipt")
                </th>
                
             
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model._otherpaymentList)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.OPid)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.R_Name)
                    </td>
                     <td>
                         @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Place)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TotalFees)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PaymentAmt)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PaymentDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Remarks)
                    </td>


                    
                   
                    <td>
                        @Html.ActionLink("Print", "PrintOtherReceipt", new { paymentid = item.OPid }, new { @class = "viewDialog" })
                    </td>
                  

                 
                </tr>
            }
        </tbody>
    </table>
</div>
